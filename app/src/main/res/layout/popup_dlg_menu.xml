<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/rounded_popup_view">
    <RelativeLayout
        android:id="@+id/relativeLayout1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:id="@+id/textView1"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_marginStart="@dimen/margin_16"
            android:layout_marginEnd="@dimen/margin_16"
            android:lineSpacingExtra="@dimen/margin_8"
            android:textColor="@color/font1"
            android:textSize="@dimen/default_font_size_18"
            android:gravity="center_vertical"
            android:text="1111"/>
    </RelativeLayout>
    <View
        android:id="@+id/viewLine"
        android:layout_below="@+id/relativeLayout1"
        android:layout_marginLeft="@dimen/margin_16"
        android:layout_marginRight="@dimen/margin_16"
        style="@style/ViewStyle_Line3"/>
    <TextView
        android:id="@+id/button1"
        android:layout_width="@dimen/default_icon_size"
        android:layout_height="@dimen/default_icon_size"
        android:layout_below="@+id/viewLine"
        android:layout_toStartOf="@+id/button2"
        android:layout_marginEnd="@dimen/margin_8"
        android:gravity="center"
        android:textStyle="bold"
        android:text="@string/string_common_cancel"
        android:clickable="true"
        android:textColor="@drawable/select_text"/>
    <TextView
        android:id="@+id/button2"
        android:layout_width="@dimen/default_icon_size"
        android:layout_height="@dimen/default_icon_size"
        android:layout_below="@+id/viewLine"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="@dimen/margin_8"
        android:gravity="center"
        android:textStyle="bold"
        android:text="@string/string_common_ok"
        android:clickable="true"
        android:textColor="@drawable/select_text"/>

</RelativeLayout>